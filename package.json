{
  "name": "cypress-typescript-cucumber",
  "version": "1.0.0",
  "description": "cypress with typescript and cucumber",
  "main": "index.js",
  "scripts": {
    "cy:open": "cross-env ./node_modules/.bin/cypress open",
    "cy:run:all": "cross-env ./node_modules/.bin/cypress run --browser chrome",
    "cy:run:e2e": "cross-env ./node_modules/.bin/cypress run --env tags=\"@e2e and not @todo\" --browser chrome",
    "cy:run:program:bdd": "cross-env ./node_modules/.bin/cypress run --env tags=\"@program/bdd and not @todo\" --browser chrome",
    "cy:run:program:suite": "cross-env ./node_modules/.bin/cypress run --spec \"cypress/test/**/*.spec.{ts,js}\" --browser chrome",
    "cy:run:program": "cross-env npm run cy:run:program:bdd && npm run cy:run:program:suite",
    "cy:run:gui": "cross-env npm run cy:run:e2e && npm run cy:run:program",
    "cy:run:api": "cross-env ./node_modules/.bin/cypress run --spec \"cypress/testApi/**/*.apiSpec.{ts,js}\"",
    "cy:run:single": "cross-env ./node_modules/.bin/cypress run --spec \"cypress/test/**/product.feature\" --browser chrome"
  },
  "author": "Mahmoud Mostafa",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@badeball/cypress-cucumber-preprocessor": "^17.2.1",
    "@cypress/webpack-preprocessor": "^5.17.1",
    "babel-loader": "^9.1.2",
    "cross-env": "^7.0.3",
    "cypress": "^12.13.0",
    "ts-loader": "^9.4.3",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.0.4",
    "webpack": "^5.85.1"
  }
}
